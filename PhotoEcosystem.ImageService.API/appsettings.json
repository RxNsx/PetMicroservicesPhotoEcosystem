{
   "Serilog": {
     "Using": [
       "Serilog.Sinks.Console",
       "Serilog.Sinks.File"
     ],
     "MinimumLevel": {
       "Default": "Information",
       "Override": {
         "Microsoft": "Warning",
         "System": "Warning"
       }
     },
     "WriteTo": [
       {
         "Name": "Console"
       },
       {
         "Name": "File",
         "Args": {
           "path": "/logs/log-.txt",
           "rollingInterval": "Day",
           "rollOnFileSizeLimit": true,
           "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact"
         }
       }
     ],
     "Enrich": [
       "FromLogContext",
       "WithMachineName",
       "WithThreadId"
     ]
 },
  "ConnectionStrings": {
    "DefaultConnection": "Server=postgre_db_image_service;Database=photoecosystem_images;User ID=admin;Password=-pl,mju7",
    "PostgreConnectionString": "Server=localhost;Database=postgres;User ID=postgres;Password=qwerty"
  },
  "AllowedHosts": "*"
}